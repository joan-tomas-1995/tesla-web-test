---
import Layout from "../layouts/Layout.astro";
import LandingHeader from "../components/LandingHeader.astro";
import HeroSection from "../components/HeroSection.astro";
import ModelS from "../components/ModelS.astro";
import ModelY from "../components/ModelY.astro";
import Model3 from "../components/Model3.astro";
import ModelX from "../components/ModelX.astro";
import Powerwall from "../components/Powerwall.astro";
import Carga from "../components/Carga.astro";
import Footer from "../components/Footer.astro";
---

<Layout title="Tesla">
  
  <LandingHeader />
  <main
    class="snap-y snap-mandatory relative w-full h-screen overflow-auto overflow-x-hidden scroll-smooth">
    <div class="snap-center"><HeroSection /></div>
    <div class="snap-center">
      <ModelS />
    </div>
    <div class="snap-center">
      <Model3 />
    </div>
    <div class="snap-center">
      <ModelX />
    </div>
    <div class="snap-center">
      <ModelY />
    </div>
    <div class="snap-center">
      <Powerwall />
    </div>
    <div class="snap-center">
      <Carga />
    </div>
    <div class="snap-center">
      <Footer />
    </div>
  </main>
</Layout>
